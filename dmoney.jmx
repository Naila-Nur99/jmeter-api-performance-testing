            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">if (prev.getResponseCode() == &quot;200&quot;) {
    try {
        // Parse the JSON response
        def jsonResponse = new groovy.json.JsonSlurper().parseText(prev.getResponseDataAsString())
        
        // Check if the token exists in the response
        if (jsonResponse.token) {
            // Store the token as a JMeter property
            props.put(&quot;token&quot;, jsonResponse.token)
            log.info(&quot;Token extracted and stored: &quot; + jsonResponse.token)
            
            // Log the token for debugging
            log.info(&quot;Token retrieved for current thread: &quot; + props.get(&quot;token&quot;))
        } else {
            log.error(&quot;Token not found in the response: &quot; + prev.getResponseDataAsString())
        }
    } catch (Exception e) {
        log.error(&quot;Failed to parse JSON response: &quot; + e.getMessage())
    }
} else {
    log.error(&quot;Login request failed with response code: &quot; + prev.getResponseCode())
}</stringProp>
